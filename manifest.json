{
  "manifest_version": 2,
  "name": "Skin Club ROI Calculator",
  "version": ".01",
  "permissions": [
    "activetab",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*.skin.club/*"  
  
  ],
  "description": "Calculates case profitability for skin.hub",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.skin.club/*"],
      "html" : ["popup.html"],
      "js": ["popup.js"]
    }
  ]
}

